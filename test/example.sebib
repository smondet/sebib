
; A simple example
(
  (id theidofthesimpleone) ; Just an advice, if you want good
                           ; hackability, don't use complex strings
                           ; for ids, juste alpha-numeric characters.
  (title "The title of the Publication")
  (authors (First Family) (First2 Family2)) ; authors is a list of couples.
  (year 1999)
  (how "Some where...")
)

; A  "complete" example:
(   (id most09complete)
    (authors ("John F." Smith) (First "Family"))
    (title "The most complete example")
    (how "By some developer")
    (year "1901")
    (keywords "Personal Data Management" "Forty-two")
